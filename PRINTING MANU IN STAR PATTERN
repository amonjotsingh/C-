
#include<stdio.h>
int main()
{
	int i,j,k,l;
	for(i=0;i<=5;i++)
	{
				printf("*");
int b;
		for(b=1;b<=i;b++)
		{
			printf(" ");
			if(b==3)
				break;
			}
		if(i<=3)
			printf("*");
			int c,d;
		for(c=5;c>=(i*2);c--)
		{
			printf(" ");

			if(i==4)
				break;
		}
		if(i<=3)
			printf("*");
		for(d=1;d<=i;d++)
		{
			printf(" ");
		}
		if(i==4)
			printf(" ");
		printf("*");
		for(j=5;j>=i;j--)
				printf(" ");
				printf("*");
				for(k=1;k<=i;k++)
				{
					if(i==3)
						printf("*");

					else
						printf(" ");

				}
				for(l=2;l<=i;l++)
				{
					if(i==3)
						printf("*");
					else
						printf(" ");

				}
				if(i!=0)
				printf("*");
				int a;

				for (a=5;a>=i;a--)
					printf(" ");

		printf(" ");
		printf("*");

		int e,f;
		for(e=1;e<=i;e++)
			printf(" ");
		printf("*");
		for(f=4;f>=i;f--)
			printf(" ");
		printf("*");
		printf(" ");
		printf("*");
		if(i==5)
			printf("*******");
		else
			printf("      *");


		printf("\n");

	}
		return 0;
}
